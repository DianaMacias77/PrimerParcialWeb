<table id="example" style="width:100%" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Ticket</th>
            <th>Correo</th>
            <th>Telefono</th>
        </tr>
    </thead>
    <tbody>
        {{#each rifa}}
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.ticket}}</td>
            <td>{{this.correo}}</td>
            <td>{{this.telefono}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>